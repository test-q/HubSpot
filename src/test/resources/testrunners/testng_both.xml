<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HubSpot Suite" thread-count="2" parallel="tests" verbose="2">

	<test name="Login Page Test -- CHROME">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.hubspot.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Login Page Test -- FF">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.hubspot.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Home Page Test -- CHROME">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.hubspot.tests.HomePageTest" />
		</classes>
	</test>
	
	<test name="Home Page Test -- FF">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.hubspot.tests.HomePageTest" />
		</classes>
	</test>
	
